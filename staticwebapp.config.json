{
  "routes": [        
        {
            "route": "/*",
            "allowedRoles": [
                "reader",
                "owner",
                "contributor"
            ]
        },
        {
          "route": "/login",
          "rewrite": "/.auth/login/aad"
        }    
    ],
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      }
    }
}
